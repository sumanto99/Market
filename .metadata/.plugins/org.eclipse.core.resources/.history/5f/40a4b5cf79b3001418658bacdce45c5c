package models;

public class Transaction {
	private int transactionId;
	private String purchaseDate;
	private int unitsSelfPurchased;
	private int unitsGiftPurchased;
	private int marketingCpc;
	private String orderCancelled;
	private String repeatPurchase;
	private String source;
	private String platform;
	private String deal_id;
	private String customerId;
	
	+----------------------+--------------+------+-----+---------+----------------+
	| Field                | Type         | Null | Key | Default | Extra          |
	+----------------------+--------------+------+-----+---------+----------------+
	| transaction_id       | int(11)      | NO   | PRI | NULL    | auto_increment |
	| purchase_date        | varchar(200) | YES  |     | NULL    |                |
	| units_self_purchased | int(11)      | YES  |     | NULL    |                |
	| units_gift_purchased | int(11)      | YES  |     | NULL    |                |
	| marketing_cpc        | double       | YES  |     | NULL    |                |
	| order_cancelled      | varchar(45)  | YES  |     | NULL    |                |
	| repeat_purchase      | varchar(45)  | YES  |     | NULL    |                |
	| source               | varchar(45)  | YES  |     | NULL    |                |
	| platform             | varchar(45)  | YES  |     | NULL    |                |
	| deal_id              | varchar(45)  | YES  | MUL | NULL    |                |
	| customer_id          | varchar(45)  | YES  | MUL | NULL    |                |
	+----------------------+--------------+------+-----+---------+----------------+

}
